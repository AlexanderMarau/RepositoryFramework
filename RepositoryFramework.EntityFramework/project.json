{
  "title": "RepositoryFramework.EntityFramework",
  "description": "A repository framework for Entity Framework Core",
  "version": "2.0.5",
  "authors": [ "Henrik Thomsen" ],
  "packOptions": {
    "projectUrl": "https://github.com/henrikbulldog/RepositoryFramework",
    "licenseUrl": "https://github.com/henrikbulldog/RepositoryFramework/blob/master/LICENSE"
  },

  "dependencies": {
    "Microsoft.EntityFrameworkCore": "1.1.0",
    "Microsoft.EntityFrameworkCore.Relational": "1.1.0",
    "RepositoryFramework.Interfaces": "2.0.3",
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    },
    "System.Data.SqlClient": "4.3.0"
  },
  "buildOptions": {
    "additionalArguments": [ "/ruleset:../RULES.ruleset", "/additionalfile:../stylecop.json" ],
    "xmlDoc": "true"
  },
  "frameworks": {
    "netcoreapp1.1": {
      "imports": [
        "dotnet5.6"
      ],
      "dependencies": {
        "System.Reflection.TypeExtensions": "4.3.0",
        "RestSharp.NetCore": "105.2.3"
      }
    },
    "net452": {
    }
  }
}