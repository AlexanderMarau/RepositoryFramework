{
  "title": "RepositoryFramework.Azure.Blob",
  "description": "A repository framework for Microsoft Azure Blob Storage",
  "version": "1.0.2",
  "authors": [ "Henrik Thomsen" ],
  "packOptions": {
    "projectUrl": "https://github.com/henrikbulldog/RepositoryFramework",
    "licenseUrl": "https://github.com/henrikbulldog/RepositoryFramework/blob/master/LICENSE"
  },

  "dependencies": {
    "WindowsAzure.Storage": "8.1.3",
    "RepositoryFramework.Interfaces": "*",
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    }
  },

  "buildOptions": {
    "additionalArguments": [ "/ruleset:../RULES.ruleset", "/additionalfile:../stylecop.json" ],
    "xmlDoc": "true"
  },

  "frameworks": {
    "netcoreapp1.1": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ]
    },
    "net452": {
    }
  }
}
