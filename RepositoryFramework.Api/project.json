{
  "title": "RepositoryFramework.Api",
  "description": "A repository framework for ReSTful API clients",
  "version": "2.0.2",
  "authors": [ "Henrik Thomsen" ],
  "packOptions": {
    "projectUrl": "https://github.com/henrikbulldog/RepositoryFramework",
    "licenseUrl": "https://github.com/henrikbulldog/RepositoryFramework/blob/master/LICENSE"
  },

  "dependencies": {
    "Newtonsoft.Json": "9.0.2-beta2",
    "RepositoryFramework.Interfaces": "2.0.3",
    "StyleCop.Analyzers": {
      "version": "1.0.0",
      "type": "build"
    }
  },

  "buildOptions": {
    "additionalArguments": [ "/ruleset:../RULES.ruleset", "/additionalfile:../stylecop.json" ],
    "xmlDoc": "true"
  },

  "frameworks": {
    "netcoreapp1.1": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ],
      "dependencies": {
        "RestSharp.NetCore": "105.2.3"
      }
    },
    "net452": {
      "dependencies": {
        "RestSharp": "105.2.3"
      }
    }
  }
}
